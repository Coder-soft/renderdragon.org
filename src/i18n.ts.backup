// Helper function to safely access browser APIs
const isServer = typeof window === 'undefined';

// Language detection configuration
const languageDetector: {
  type: 'languageDetector';
  async: boolean;
  init: () => void;
  detect: (callback: (lng: string) => void) => void;
  cacheUserLanguage?: (lng: string) => void;
} = {
  type: 'languageDetector',
  async: true,
  init: () => {},
  detect: (callback: (lng: string) => void) => {
    // Default fallback
    let detectedLang = 'en';
    const supportedLangs = ['en', 'es', 'fr', 'nl'];
    
    if (!isServer) {
      // 1. Check URL parameter first
      const urlParams = new URLSearchParams(window.location.search);
      const urlLang = urlParams.get('lng');
      
      if (urlLang && supportedLangs.includes(urlLang)) {
        detectedLang = urlLang;
      } else {
        // 2. Check localStorage
        try {
          const storedLang = localStorage.getItem('i18nextLng');
          if (storedLang && supportedLangs.includes(storedLang)) {
            detectedLang = storedLang;
          } else {
            // 3. Check cookie
            const cookieLang = Cookies.get('i18next');
            if (cookieLang && supportedLangs.includes(cookieLang)) {
              detectedLang = cookieLang;
            } else {
              // 4. Detect browser/region language
              const browserLang = navigator.language || (navigator as any).userLanguage || 'en';
              const langCode = browserLang.split('-')[0].toLowerCase();
              
              // Region-specific overrides (key order: en, es, fr, nl)
              const regionSpecificLangs: Record<string, string> = {
                // English regions
                'ph': 'en', // Philippines
                'sg': 'en', // Singapore
                'my': 'en', // Malaysia
                'za': 'en', // South Africa
                'ng': 'en', // Nigeria
                'ke': 'en', // Kenya
                'gh': 'en', // Ghana
                
                // Spanish regions
                'ar': 'es', // Argentina
                'mx': 'es', // Mexico
                'co': 'es', // Colombia
                'cl': 'es', // Chile
                'pe': 'es', // Peru
                've': 'es', // Venezuela
                'ec': 'es', // Ecuador
                'gt': 'es', // Guatemala
                'cu': 'es', // Cuba
                'do': 'es', // Dominican Republic
                'bo': 'es', // Bolivia
                'hn': 'es', // Honduras
                'py': 'es', // Paraguay
                'sv': 'es', // El Salvador
                'ni': 'es', // Nicaragua
                'cr': 'es', // Costa Rica
                'pa': 'es', // Panama
                'uy': 'es', // Uruguay
                'gq': 'es', // Equatorial Guinea
                
                // French regions
                'ca': 'fr', // Canada (Quebec)
                'ch': 'fr', // Switzerland (French part)
                'lu': 'fr', // Luxembourg
                'cm': 'fr', // Cameroon
                'ci': 'fr', // Ivory Coast
                'sn': 'fr', // Senegal
                'cd': 'fr', // DR Congo
                'ma': 'fr', // Morocco
                'tn': 'fr', // Tunisia
                
                // Dutch regions
                'be': 'nl', // Belgium (Flanders)
                'sr': 'nl', // Suriname
                'aw': 'nl', // Aruba
                'cw': 'nl', // Curaçao
                'sx': 'nl', // Sint Maarten
              };

              // Check if we have a region-specific override
              const region = browserLang.split('-')[1]?.toLowerCase();
              if (region && region in regionSpecificLangs) {
                detectedLang = regionSpecificLangs[region];
              } else if (supportedLangs.includes(langCode)) {
                detectedLang = langCode;
              }
            }
          }
        } catch (e) {
          console.warn('Failed to detect language, falling back to English', e);
        }
      }
      
      // Save detected language
      if (!isServer) {
        try {
          localStorage.setItem('i18nextLng', detectedLang);
          Cookies.set('i18next', detectedLang, { 
            expires: 365,
            sameSite: 'Lax',
            secure: window.location.protocol === 'https:',
            path: '/'
          });
        } catch (e) {
          console.warn('Failed to save language preference', e);
        }
      }
    }
    
    callback(detectedLang);
  },
  cacheUserLanguage: (lng: string) => {
    if (isServer) return;
    
    // Save language preference to both localStorage and cookie
    try {
      localStorage.setItem('i18nextLng', lng);
      Cookies.set('i18next', lng, { 
        expires: 365,
        sameSite: 'Lax',
        secure: window.location.protocol === 'https:',
        path: '/'
      });
    } catch (e) {
      console.warn('Failed to save language preference', e);
    }
  }
};

// Configure i18next
const initI18n = async (): Promise<I18nType> => {
  const detectionOptions = {
    order: ['querystring', 'localStorage', 'cookie', 'navigator'],
    caches: ['localStorage', 'cookie'],
    lookupQuerystring: 'lng',
    lookupLocalStorage: 'i18nextLng',
    lookupCookie: 'i18next',
  };

  await i18n
    .use(initReactI18next)
    .use(languageDetector)
    .init({
      resources,
      fallbackLng: 'en',
      supportedLngs: ['en', 'es', 'fr', 'nl'],
      nonExplicitSupportedLngs: true,
      load: 'languageOnly',
      cleanCode: true,
      
      // Detection settings
      detection: detectionOptions,
      
      // Cache settings
      saveMissing: true,
      missingKeyHandler: (lngs, ns, key) => {
        console.warn(`Missing translation: ${key} for language ${lngs}`);
      },
      
      // Language matching
      lowerCaseLng: true,
      
      // Interpolation
      interpolation: {
        escapeValue: false,
      },
      
      // React settings
      react: {
        useSuspense: true,
        bindI18n: 'languageChanged',
        bindI18nStore: '',
        transEmptyNodeValue: '',
        transSupportBasicHtmlNodes: true,
        transKeepBasicHtmlNodesFor: ['br', 'strong', 'i', 'p', 'b', 'span'],
      },
    });

  // Handle language changes
  if (!isServer) {
    i18n.on('languageChanged', (lng: string) => {
      // Update HTML lang attribute
      document.documentElement.setAttribute('lang', lng);
      
      // Save to both localStorage and cookie
      try {
        localStorage.setItem('i18nextLng', lng);
        const isHttps = window.location.protocol === 'https:';
        const cookieOptions: Cookies.CookieAttributes = { 
          expires: 365,
          sameSite: 'Lax' as const,
          secure: isHttps,
          path: '/'
        };
        Cookies.set('i18next', lng, cookieOptions);
      } catch (e) {
        console.warn('Failed to save language preference', e);
      }
    });
  }

  return i18n;
};

// Initialize i18n and export types
export const i18nInstance = i18n;

// Initialize i18n and handle potential errors
let isInitialized = false;

export const initializeI18n = async (): Promise<I18nType> => {
  if (!isInitialized) {
    try {
      await initI18n();
      isInitialized = true;
    } catch (error) {
      console.error('Failed to initialize i18n:', error);
      // Fallback to default initialization
      await i18n.init({
        resources,
        fallbackLng: 'en',
        supportedLngs: ['en', 'es', 'fr', 'nl'],
        load: 'languageOnly',
      });
    }
  }
  return i18n;
};

